{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "env": [
      {
        "key": "NIXPACKS_VERSION",
        "value": "1.29.1"
      }
    ]
  },
  "deploy": {
    "startCommand": "gunicorn src.main:app --bind 0.0.0.0:$PORT --workers 4 --timeout 120"
  }
}
